y.from(menu.querySelectorAll(\"input\")).find(\r\n            (cb) => cb.value === val\r\n          );\r\n          if (targetCb) {\r\n            targetCb.checked = false;\r\n            targetCb.dispatchEvent(new Event(\"change\"));\r\n          }\r\n        });\r\n      });\r\n    });\r\n  });\r\n}\r\n\r\nonMounted(() => {\r\n  setupMultiSelect(propertyDropdown);\r\n  setupMultiSelect(residentialDropdown);\r\n});\r\n</script>"],"version":3}